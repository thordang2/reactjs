{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "react/destructuring-assignment": ["<enabled>", "always", { "ignoreClassFields": "<boolean>"}]
  },
  "parser": "babel-eslint"
}